{
  "$schema": "vscode://schemas/color-theme",
  "author": "Dan Hedgecock",
  "name": "Radical",
  "colorSpaceName": "sRGB",
  "semanticClass": "theme.dark.radical",
  "colors": {
    "activityBar.background": "#212b36",
    "activityBar.dropBackground": "#f86c8a99",
    "activityBar.foreground": "#befeee",
    "activityBarBadge.background": "#fe6083",
    "activityBarBadge.foreground": "#befeee",
    "badge.background": "#fe6083",
    "badge.foreground": "#befeee",
    "focusBorder": "#0000",
    "foreground": "#85a5a0",
    "widget.shadow": "#f86c8a1a",
    "button.background": "#fe6083",
    "button.foreground": "#befeee",
    "button.hoverBackground": "#e25777",
    "diffEditor.insertedTextBackground": "#43fdd533",
    "diffEditor.removedTextBackground": "#fe608233",
    "editor.background": "#212b36",
    "editor.foreground": "#85a5a0",
    "editorCursor.foreground": "#fe6083",
    "editorError.foreground": "#ff1767",
    "editorWarning.foreground": "#ffd000",
    "editorRuler.foreground": "#f86c8a99",
    "editorGroup.border": "#1c242c",
    "editorGroupHeader.tabsBackground": "#212b36",
    "editorGroupHeader.tabsBorder": "#1c242c",
    "editorGutter.addedBackground": "#a3ff57",
    "editorGutter.modifiedBackground": "#ffb000",
    "editorGutter.deletedBackground": "#ff427b",
    "editorOverviewRuler.errorForeground": "#ff1767",
    "editorOverviewRuler.warningForeground": "#ffd000",
    "editorOverviewRuler.infoForeground": "#93E0E3",
    "editorOverviewRuler.modifiedForeground": "#ffb000",
    "editorOverviewRuler.addedForeground": "#a3ff57",
    "editorOverviewRuler.deletedForeground": "#ff427b",
    "editorWidget.background": "#262b4be6",
    "extensionButton.prominentForeground": "#befeee",
    "extensionButton.prominentBackground": "#fe6083",
    "extensionButton.prominentHoverBackground": "#e25777",
    "gitDecoration.addedResourceForeground": "#a3ff57",
    "gitDecoration.modifiedResourceForeground": "#ffb000",
    "gitDecoration.deletedResourceForeground": "#ff427b",
    "gitDecoration.untrackedResourceForeground": "#c8ff00",
    "gitDecoration.ignoredResourceForeground": "#405c5f",
    "gitDecoration.conflictingResourceForeground": "#ff428a",
    "gitDecoration.submoduleResourceForeground": "#cc6796",
    "input.background": "#1c2b3b",
    "input.border": "#befeee80",
    "list.hoverBackground": "#f86c8a0D",
    "list.hoverForeground": "#85a5a0",
    "list.focusBackground": "#f86c8a0d",
    "list.focusForeground": "#fe6083",
    "list.activeSelectionBackground": "#f86c8a1A",
    "list.activeSelectionForeground": "#fe6083",
    "list.inactiveSelectionBackground": "#f86c8a1A",
    "list.inactiveSelectionForeground": "#fe6083",
    "list.dropBackground": "#f86c8a99",
    "list.highlightForeground": "#befeee",
    "list.errorForeground": "#ff1767",
    "list.warningForeground": "#ffd000",
    "panel.background": "#212b36",
    "panel.border": "#1c242c",
    "panel.dropBackground": "#f86c8a99",
    "panelTitle.activeBorder": "#fe6083",
    "panelTitle.activeForeground": "#fe6083",
    "panelTitle.inactiveForeground": "#85a5a0",
    "progressBar.background": "#602976",
    "scrollbar.shadow": "#f86c8a1a",
    "scrollbarSlider.background": "#f86c8a1a",
    "scrollbarSlider.hoverBackground": "#f86c8a40",
    "scrollbarSlider.activeBackground": "#f86c8a66",
    "sideBar.background": "#212b36",
    "sideBar.foreground": "#919ca8",
    "sideBar.border": "#1c242c",
    "sideBar.dropBackground": "#f86c8a99",
    "sideBarTitle.foreground": "#85a5a0",
    "sideBarSectionHeader.background": "#212b36",
    "sideBarSectionHeader.foreground": "#85a5a0",
    "statusBar.background": "#262b4b",
    "statusBar.foreground": "#85a5a0",
    "statusBar.border": "#602976",
    "statusBar.debuggingBackground": "#262b4b",
    "statusBar.debuggingForeground": "#befeee",
    "statusBar.noFolderBackground": "#262b4b",
    "statusBar.noFolderForeground": "#85a5a0",
    "statusBar.noFolderBorder": "#602976",
    "statusBarItem.prominentBackground": "#602976",
    "statusBarItem.prominentHoverBackground": "#913eb4",
    "statusBarItem.hoverBackground": "#602976",
    "statusBarItem.activeBackground": "#602976",
    "tab.activeBorder": "#fe6083",
    "tab.activeBackground": "#212b36",
    "tab.activeForeground": "#fe6083",
    "tab.inactiveBackground": "#212b36",
    "tab.inactiveForeground": "#85a5a0",
    "textBlockQuote.background": "#262b4b",
    "textBlockQuote.border": "#85a5a0",
    "textLink.foreground": "#befeee",
    "textLink.activeForeground": "#a4dcd0",
    "textPreformat.foreground": "#fe6083",
    "titleBar.activeBackground": "#262b4b",
    "titleBar.activeForeground": "#fe6083",
    "titleBar.border": "#602976",
    "welcomePage.buttonBackground": "#262b4b",
    "welcomePage.buttonHoverBackground": "#222745",
    "walkThrough.embeddedEditorBackground": "#1e2732"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#ab9bab",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#f0f0f0"
      }
    },
    {
      "scope": "entity",
      "settings": {
        "foreground": "#e8e8e8"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#e0e0e0",
        "fontStyle": "italic bold underline"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#d9d9d9"
      }
    },
    {
      "scope": "markup",
      "settings": {
        "foreground": "#d1d1d1"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#c9c9c9"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#bfbfbf"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#b5b5b5"
      }
    }
  ]
}