    {
      "scope": "comment",
      "settings": {
        "foreground": "#a8c0c2",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#fa61b8"
      }
    },
    {
      "scope": "entity",
      "settings": {
        "foreground": "#a6e2e0"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#ff427b",
