  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#64749A",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#fa61b8"
      }
    },
    {
      "scope": "entity",
      "settings": {
        "foreground": "#aadedc"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#ff427b",
        "fontStyle": "italic bold underline"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#d5358f"
      }
    },
    {
      "scope": "markup",
      "settings": {
        "foreground": "#85a5a0"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#ff85a1"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#a9fef7"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#8da4a5"
      }
    },
    {
      "scope": "variable",
      "settings": {
